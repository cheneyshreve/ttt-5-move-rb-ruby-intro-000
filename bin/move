#!/usr/bin/env ruby

require_relative '../lib/move.rb'

# Code your CLI Here
print "Welcome to Tic Tac Toe!"
board = display_board([" "," "," "," "," "," "," "," "," "])
print "Where would you like to go?"
user_input = gets.strip.to_i
index = input_to_index(user_input)
board = move(board,index,char)
display_board(board)
